	<subDataset name="VendasPeriodo" uuid="91929307-11a8-4450-aa66-a0256be0903b">
		<parameter name="DataInicial" class="java.util.Date">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="DataFinal" class="java.util.Date">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
     funcionario.nome AS Vendedor,
     tipo_venda AS Tipo,
     data_venda AS Data,
     valor_total AS Valor
FROM venda
INNER JOIN funcionario
ON funcionario.id = venda.id_funcionario]]>
		</queryString>
		<field name="Data" class="java.sql.Date"/>
		<field name="Valor" class="java.lang.Double"/>
		<field name="Tipo" class="java.lang.String"/>
		<field name="Vendedor" class="java.lang.String"/>
		<group name="Vendedor">
			<groupExpression><![CDATA[$F{Vendedor}]]></groupExpression>
		</group>
		<group name="Data">
			<groupExpression><![CDATA[$F{Data}]]></groupExpression>
		</group>
	</subDataset>
	<parameter name="DataInicial" class="java.util.Date"/>
	<parameter name="DataFinal" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT
     venda.`id` AS venda_id,
     venda.`data_venda` AS venda_data_venda,
     venda.`valor_total` AS venda_valor_total,
     venda.`tipo_venda` AS venda_tipo_venda,
     funcionario.`nome` AS funcionario_nome
FROM
     `funcionario` funcionario INNER JOIN `venda` venda ON funcionario.`id` = venda.`id_funcionario`
WHERE
     venda.data_venda BETWEEN $P{DataInicial}and$P{DataFinal}
GROUP BY
     funcionario_nome
ORDER BY
     funcionario.`nome` ASC,
     venda.`data_venda` ASC]]>
	</queryString>
	<field name="venda_id" class="java.lang.Integer"/>
	<field name="venda_data_venda" class="java.sql.Timestamp"/>
	<field name="venda_valor_total" class="java.lang.Double"/>
	<field name="venda_tipo_venda" class="java.lang.String"/>
	<field name="funcionario_nome" class="java.lang.String"/>
